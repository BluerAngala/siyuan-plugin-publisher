{
  "name": "sy-post-publisher",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "deploy": "bash deploy.sh",
    "deploy-win": "pwsh deploy.ps1",
    "test": "node --experimental-specifier-resolution=node --loader ts-node/esm test/test.ts",
    "testEsm": "ts-node-esm --experimental-specifier-resolution=node test/test.ts",
    "vdev": "vercel dev --listen 5005",
    "vbuild": "vercel build",
    "wd": "yarn deploy-win"
  },
  "dependencies": {
    "@octokit/core": "^4.0.4",
    "copy-to-clipboard": "^3.3.1",
    "element-plus": "^2.2.11",
    "express": "^4.18.1",
    "isomorphic-fetch": "^3.0.0",
    "js-base64": "^3.7.2",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.7",
    "shorthash2": "^1.0.3",
    "transliteration": "^2.3.5",
    "uuid": "^8.3.2",
    "vue": "^3.2.37",
    "vue-i18n": "9",
    "xmlrpc": "^1.3.2"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.2",
    "@vitejs/plugin-vue": "^3.0.0",
    "dotenv": "^16.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-require-transform": "^1.0.3",
    "vue-tsc": "^0.38.4"
  }
}

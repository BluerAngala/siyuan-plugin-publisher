{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "serve": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "build": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dockerBuild": {
      "cache": false
    },
    "vercelBuild": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "siyuanBuild": {
      "cache": false
    },
    "package": {
      "cache": false
    },
    "syncVersion": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "preview": {
      "dependsOn": ["^build"]
    }
  }
}
